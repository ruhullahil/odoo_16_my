<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="approval_view_sequence" model="ir.sequence">
            <field name="name">NT Approval</field>
            <field name="code">nt.approval.seq</field>
            <field name="prefix">AP</field>
            <field name="padding">1</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>
        <record id="approval_order_view_sequence" model="ir.sequence">
            <field name="name">NT Order</field>
            <field name="code">nt.approval.order.seq</field>
            <field name="prefix">AO</field>
            <field name="padding">4</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>

    </data>
</odoo>