<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_partner_credit_form_view_form" model="ir.ui.view">
        <field name="name">res_partner_credit_form_view_form</field>
        <field name="model">res.partner</field>
        <field name="arch" type="xml">
            <form string="res_partner_credit_form_form">
                <sheet>
                    <div class="oe_title">
                        <h5>
                            <label for="member_id"/>
                            <field name="member_id" />
                        </h5>
                        <h5>
                            <label for="data_of_add"/>
                            <field name="data_of_add" />
                        </h5>
                    </div>
                    <group>
                        <group name="info" string="Info">
                            <field name="name"/>
                            <field name="husband_or_father_name"/>
                            <field name="mother_name"/>
                            <field name="current_address"/>
                            <field name="permanent_address"/>
                            <field name="nid_no"/>
                            <field name="occupation"/>
                            <field name="mobile"/>
                        </group>
                        <group>

                        </group>

                    </group>
                    <group>

                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>