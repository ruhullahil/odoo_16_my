<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_credit_balance_move" model="ir.sequence">
            <field name="name">Balance Move</field>
            <field name="code">credit.balance.move</field>
            <field name="prefix">CBM/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_account_credit_entry" model="ir.sequence">
            <field name="name">Credit Entry</field>
            <field name="code">account.credit.entry</field>
            <field name="prefix">ACE/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_account_credit_payment" model="ir.sequence">
            <field name="name">Credit Payment</field>
            <field name="code">account.credit.payment</field>
            <field name="prefix">ACP/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>